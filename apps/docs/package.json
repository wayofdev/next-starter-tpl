    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@wayofdev/browserslist-config": "^1.1.0",
    "@wayofdev/eslint-config-custom": "workspace:*",
    "es-check": "^7.0.1",
    "eslint": "^8.32.0",
