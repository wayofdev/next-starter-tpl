{
  "name": "@wayofdev/storybook",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "build": "storybook build",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "dev": "storybook dev -p 443 --disable-telemetry",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx --cache --cache-location ../../.cache/eslint/storybook.eslintcache",
    "lint:dist": "es-check -v",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx --fix --cache-location ../../.cache/eslint/storybook.eslintcache",
    "lint:types": "tsc --project ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@wayofdev/ui": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.0.21",
    "@storybook/addon-interactions": "7.0.21",
    "@storybook/addon-links": "7.0.21",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addon-viewport": "^7.0.0-rc.5",
    "@storybook/blocks": "7.0.21",
    "@storybook/cli": "^7.0.0-rc.5",
    "@storybook/react": "7.0.21",
    "@storybook/react-vite": "7.0.21",
    "@storybook/testing-library": "0.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@vitejs/plugin-react-swc": "3.3.2",
    "@wayofdev/eslint-config-bases": "2.0.7",
    "@wayofdev/postcss-config": "2.0.8",
    "autoprefixer": "^10.4.14",
    "eslint": "8.36.0",
    "postcss": "8.4.24",
    "postcss-100vh-fix": "1.0.2",
    "postcss-cli": "10.1.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-normalize": "10.0.1",
    "postcss-preset-env": "8.5.0",
    "postcss-reporter": "7.0.5",
    "storybook": "7.0.21",
    "tailwindcss": "3.2.7",
    "typescript": "5.0.2",
    "vite": "4.3.9"
  }
}
